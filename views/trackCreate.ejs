<%- include('partials/header', {title:"Create Track"}) -%>

<h2>Create a New Track!</h2>

<form action="/trackCreate" method="POST">
  <label for="title">Title:</label>
  <input type="text" id="title" name="title" value=""><br>
  <label for="date">Release Date:</label>
  <input type="text" id="date" name="date" value=""><br>
  <label for="years">Musicians:</label>
  <input type="text" id="musicians" name="musicians" value=""><br>
  <label for="label">Label:</label>
  <input type="text" id="label" name="label" value=""><br>
  <label for="genre">Genre:</label>
  <input type="text" id="genre" name="genre" value=""><br>
  <label for="count">Commentary:</label>
  <input type="text" id="commentary" name="commentary" value=""><br>
  <br><br>
  <input type="submit" value="Create a new track!">

</form>

<%- include('partials/footer') -%>
